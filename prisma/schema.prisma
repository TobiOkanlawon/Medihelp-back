// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:../db/database.db"
}

generator client {
  provider = "prisma-client-js"
}

model Student {
  clinicID    String    @id
  name        String    
}

model Doctor {
  email       String    @id
  firstName   String
  lastName    String
}

model Pharmacist {
  email       String    @id
  firstName   String
  lastName    String
}